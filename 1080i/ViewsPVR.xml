						<width>6-col-gutter</width>
						<font>itemTitle</font>
						<textcolor>selected</textcolor>
						<shadowcolor>-</shadowcolor>
						<label>$INFO[ListItem.nextTitle]</label>
					</control>
				</itemlayout>
				<focusedlayout height="1-row" width="20-col">
