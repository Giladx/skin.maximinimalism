					</control>
					<control type="label">
						<posx>12-col</posx>
						<posy>0</posy>
						<width>84</width>
						<font>itemTitle</font>
						<textcolor>black</textcolor>
						<selectedcolor>black</selectedcolor>
						<shadowcolor>-</shadowcolor>
						<label>$INFO[ListItem.nextstarttime]</label>
					</control>
					<control type="label">
						<posx>948</posx>
